<h1>New book</h1>
<%= form_with model: @book do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
  <%= f.text_area :body %>
  <%= f.submit 'Create Book' %>
<% end %>

<h2>Books</h2>

<% @books.each do |book| %>
<div>
  <p>Title:<%= book.title %></p>
  <p>Body:<%= book.body %></p>
</div>
<% end %>